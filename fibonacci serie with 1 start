import java.util.Scanner;
public class Main{
    public static void main(String [] args)
    {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int a=1;
int b=0;
int c=1;
// System.out.print(a+""+b);
for(int i=0;i<num;i++)
{
    c=a+b;
    System.out.print(c);
    a=b;
    b=c;
}
    }
}

import java.util.Scanner;

public class Main{//folder name and class name should be same tabhi run hoga
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enetr a nnumber");
        int num=sc.nextInt();
        int rev=0;
        while(num!=0)
        {
        int t=num%10;
            rev=rev*10+t;
            num=num/10;
            
        }
        System.out.println("revrse number is "+rev);
    }
}
